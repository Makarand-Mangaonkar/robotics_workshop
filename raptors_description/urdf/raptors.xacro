<link name="right_wheel_v3_1">
  <inertial>
    <origin xyz="1.269686219929395e-17 -0.020000000000000018 0.0" rpy="0 0 0"/>
    <mass value="5.623069146418319"/>
    <inertia ixx="0.008763" iyy="0.016026" izz="0.008763" ixy="0.0" iyz="0.0" ixz="-0.0"/>
