    <geometry>
      <mesh filename="package://raptors_description/meshes/depth_camera_v1_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="steel_satin"/>
  </visual>
  <collision>
    <origin xyz="-0.28 0.0 -0.0805" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://raptors_description/meshes/depth_camera_v1_1.stl" scale="0.001 0.001 0.001"/>
